{
  "name": "eslint-config-strict",
  "version": "5.0.0",
  "description": "ESLint sharable config for strict linting",
  "main": "es6.js",
  "eslintConfig": {
    "env": {
      "node": true
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ]
    }
  },
  "keywords": [
    "strict",
    "style checker",
    "code style",
    "code checker",
    "code linter",
    "style linter",
    "simple",
    "policy",
    "style",
    "code",
    "lint",
    "eslint",
    "eslintconfig",
    "jshint",
    "jscs",
    "hint",
    "enforce",
    "check",
    "verify",
    "quality",
    "checker",
    "code standards",
    "strict style"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/keithamus/eslint-config-strict.git"
  },
  "dependencies": {
    "eslint": "^1.3.1",
    "eslint-plugin-filenames": "^0.1.2"
  },
  "author": "Keith Cirkel <npm@keithcirkel.co.uk> (http://keithcirkel.co.uk/)",
  "license": "MIT",
  "scripts": {}
}
